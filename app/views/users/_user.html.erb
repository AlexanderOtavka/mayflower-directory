<li>
    <p>Name: <%= user.name %> </p>
    <p>Email: <%= user.email %></p>
   <% if current_user.admin %>
   <%= link_to "Delete", user, method: :delete,
                                  data: { confirm: "Are you sure you want to delete this user?" } unless user.admin %> |
   <%= link_to "Reset password", edit_password_user_path(user) %> |
   <%= link_to "Edit User Information", edit_info_user_path(user) %>
  <% end %>
  
</li>
